<!DOCTYPE html>
<html>
  <head>
    <title>Gmaps</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'https://maps.googleapis.com/maps/api/js?key='+Rails.application.credentials.google_maps_api_key, defer: true  %>
    <%# <%= script async defer src='https://maps.googleapis.com/maps/api/js?key='+Rails.application.credentials.google_maps_api_key+'&callback=initMap'
  type="text/javascript"></script> %>
    <%= yield(:head_tags) %>
  </head>

  <body>
  <h1><%= link_to 'HOME', root_path %></h1>
    <%= yield %>
  </body>
</html>
